{
  "name": "redisbox",
  "version": "0.0.1",
  "author": "zwtao90@gmail.com",
  "description": "Redis 图形化客户端",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview"
  },
  "build": {
    "productName": "redisbox",
    "appId": "com.example.redisbox",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/open.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "electron": "^16.0.1",
    "vue": "^3.2.22"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.10.0",
    "chalk": "^4.1.2",
    "electron": "^16.0.1",
    "electron-debug": "^3.2.0",
    "vite": "^2.6.14"
  }
}
